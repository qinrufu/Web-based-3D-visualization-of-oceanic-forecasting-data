body::-webkit-scrollbar {
    display: none;
}

body {
    -moz-user-select: none;
    -khtml-user-select: none;
    user-select: none;
}

.menu dl,
.nav,
.mainHeader .logo,
.nav .navCon dl {
    float: left;
}


/*.nav,*/

.systemConfig,
.userInfo,
.mediaNav {
    float: right;
}

.menu .secList {
    clear: both;
}

.scrollBar_y,
.mediaNav,
.nav .navCon,
