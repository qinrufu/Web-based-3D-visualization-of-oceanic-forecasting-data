.printBar { 
    position: absolute;
    right: 20px;
    top: 144px;
    z-index: 999;
    height: 45px;
    padding: 5px 10px 5px 10px;
    font-size: 14px;
    background-color: #ffffff;
    border-radius:5px;
}
 
.dark .printBar {
    color: #ffffff;
    background-color: rgba(63, 72, 84, 0.6);
}

/*******地图对比中间线样式******/
.cesium-map-contrast-slider {
    position: absolute;
    left: 50%;
    top: 0px;
    background-color: #D3D3D3;
    width: 2px;
    height: 100%;
    z-index: 9999;
}

    .cesium-map-contrast-slider:hover {
        cursor: ew-resize;
    }
